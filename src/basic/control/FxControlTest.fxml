<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="389.0" prefWidth="517.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="basic.control.FxControlTestController">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="4.0">
         <children>
            <Label text="이  름: ">
               <HBox.margin>
                  <Insets bottom="20.0" />
               </HBox.margin>
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <TextField fx:id="name" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0" spacing="4.0">
         <children>
            <Label text="성  별: ">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <RadioButton fx:id="male" mnemonicParsing="false" selected="true" text="남">
               <toggleGroup>
                  <ToggleGroup fx:id="gender" />
               </toggleGroup>
            </RadioButton>
            <RadioButton fx:id="female" mnemonicParsing="false" text="여" toggleGroup="$gender" />
         </children>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </HBox>
      <HBox prefHeight="35.0" prefWidth="497.0" spacing="4.0">
         <children>
            <Label text="취  미: ">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <CheckBox fx:id="ck1" mnemonicParsing="false" text="여행" />
            <CheckBox fx:id="ck2" mnemonicParsing="false" text="등산" />
            <CheckBox fx:id="ck3" mnemonicParsing="false" text="독서" />
            <CheckBox fx:id="ck4" mnemonicParsing="false" text="바둑" />
            <CheckBox fx:id="ck5" mnemonicParsing="false" text="장기" />
            <CheckBox fx:id="ck6" mnemonicParsing="false" text="게임" />
            <CheckBox fx:id="ck7" mnemonicParsing="false" text="테니스" />
            <CheckBox fx:id="ck8" mnemonicParsing="false" text="배드민턴" />
         </children>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </HBox>
      <Button fx:id="btn" mnemonicParsing="false" onAction="#printInfo" prefHeight="37.0" prefWidth="85.0" text="보  기">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Button>
      <TextArea fx:id="ta" prefHeight="512.0" prefWidth="517.0" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
