<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="676.0" prefWidth="591.0" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="basic.mvc.main.FxmlMemberController">
   <children>
      <HBox alignment="CENTER" prefHeight="0.0" prefWidth="200.0">
         <children>
            <Label text="   회원 ID : ">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="memId" prefHeight="30.0" prefWidth="250.0" promptText="회원 ID">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="0.0" prefWidth="200.0">
         <children>
            <Label text="회원 이름 : ">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="memName" prefHeight="30.0" prefWidth="250.0" promptText="회원 이름">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="0.0" prefWidth="200.0">
         <children>
            <Label text="회원 전화 : ">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="memTel" prefHeight="30.0" prefWidth="250.0" promptText="회원 전화">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="0.0" prefWidth="200.0">
         <children>
            <Label text="회원 주소 : ">
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
            <TextField fx:id="memAddr" prefHeight="30.0" prefWidth="250.0" promptText="회원 주소">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="44.0" prefWidth="473.0" spacing="10.0">
         <children>
            <Button fx:id="btnAdd" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#memAdd" prefHeight="40.0" prefWidth="55.0" text="추가" />
            <Button fx:id="btnEdit" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#memEdit" prefHeight="40.0" prefWidth="55.0" text="수정" />
            <Button fx:id="btnDel" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#memDelete" prefHeight="40.0" prefWidth="55.0" text="삭제" />
            <Button fx:id="btnCheck" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#memCheck" prefHeight="40.0" prefWidth="55.0" text="확인" />
            <Button fx:id="btnCancel" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#memCancel" prefHeight="40.0" prefWidth="55.0" text="취소" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
      <TableView fx:id="table" onMouseClicked="#tableClick" prefHeight="424.0" prefWidth="473.0">
        <columns>
          <TableColumn fx:id="tableId" prefWidth="103.0" resizable="false" text="회원 ID" />
          <TableColumn fx:id="tableName" prefWidth="126.0" resizable="false" text="회원 이름" />
            <TableColumn fx:id="tableTel" prefWidth="179.0" resizable="false" text="회원 전화" />
            <TableColumn fx:id="tableAddr" prefWidth="162.0" resizable="false" text="회원 주소" />
        </columns>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
