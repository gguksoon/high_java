<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
	PUBLIC "-//ibatis.apache.org/DTD SQL Map 2.0//EN"
	"http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="zipSearch">
	<typeAlias alias="zipVo" type="zipSearch.vo.ZipVO"/>
	  
	<!-- select 연습 -->
	<select id="getSearchDong" resultClass="zipVo" parameterClass="String">
		select *
		  from ziptb
		 where dong like '%' || #dong# || '%'
	</select>

	<select id="getSearchCode" resultClass="zipVo" parameterClass="String">
		select *
		  from ziptb
		 where zipcode like '%' || #code# || '%'
	</select>


</sqlMap>












